{
  "expo": {
    "name": "borgapp",
    "slug": "borgapp",
    "scheme": "borgapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/btrfly-logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bgonzalez4717.borgapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bgonzalez4717.borgapp"
    },
    "web": {
      "favicon": "./assets/btrfly-logo.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c80c2015-1b54-4837-ac97-2c8c8b811ec3"
      }
    }
  }
}
