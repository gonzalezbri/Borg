{
  "expo": {
    "name": "Therapy Helpers",
    "slug": "therapy-helpers",
    "scheme": "therapy-helpers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/btrfly-logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main.png",
      "resizeMode": "contain",
      "backgroundColor": "#162238"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bgonzalez4717.borgapp",
      "buildNumber": "5",
      "infoPlist": {
        "CFBundleDisplayName": "Therapy Helpers"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bgonzalez4717.borgapp"
    },
    "web": {
      "favicon": "./assets/btrfly-logo.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6acf4c60-90e5-4e50-a35f-6a2bc4bf0b6a"
      }
    }
  }
}
